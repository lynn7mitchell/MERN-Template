hboard\r\n\r\n        axios\r\n          .post(\"api/user/login\", newUser)\r\n          .then((res) => {\r\n            // if (res.data.token) {\r\n            //   const { token } = res.data;\r\n            //   localStorage.setItem(\"example-app\", token);\r\n            //   setAuthToken(token);\r\n            // }\r\n            // setRedirect(true),\r\n            //   setErrors({\r\n            //     errors: {},\r\n            //   });\r\n          })\r\n          .catch((err) => {\r\n            console.error(err.res.data);\r\n            setErrors(err.res.data);\r\n          });\r\n      })\r\n      .catch((err) => {\r\n        console.error(err.res.data);\r\n        setErrors(err.res.data);\r\n      });\r\n  };\r\n  if (redirect) {\r\n    return <Redirect to=\"/dashboard\" />;\r\n  }\r\n  return( \r\n  <div>\r\n     <Link to={{ pathname: \"/\" }}>\r\n        <i className=\"material-icons back-button\">arrow_back</i>\r\n      </Link>\r\n      <h3>SIGN UP</h3>\r\n      \r\n  </div>);\r\n}\r\n","C:\\Users\\lynn7\\Desktop\\newTemplate\\client\\src\\pages\\NoMatch.js",[],"C:\\Users\\lynn7\\Desktop\\newTemplate\\client\\src\\utils\\Authenticate.js",[],"C:\\Users\\lynn7\\Desktop\\newTemplate\\client\\src\\pages\\Dashboard.js",["71"],"import React, { Component } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport setAuthToken from \"../utils/setAuthtoken\";\r\nimport axios from \"axios\";\r\n// import io from 'socket.io-client';\r\n\r\nexport class Dashboard extends Component {\r\n  state = {\r\n    user: {}\r\n  };\r\n  \r\n  componentWillMount() {\r\n    const token = localStorage.getItem(\"example-app\");\r\n\r\n    if (token) {\r\n      setAuthToken(token);\r\n    }\r\n\r\n    axios\r\n      .get(\"api/user\")\r\n      .then(response => {\r\n        this.setState({\r\n          user: response.data\r\n        });\r\n      })\r\n      .catch(err => console.log(err.response));\r\n\r\n\r\n\r\n      \r\n  }\r\n\r\n  // componentDidMount(){\r\n 